{
	"reader":{
		"name":"BasicExampleFile",
		"type":"FileReader",
		"file":"",
		"delim":"|",
		"header":[
			"id",
			"value"
		],
		"has_header":true,
		"id_field":"id"
	},
	"metrics":[
		{
			"type":"FieldCount",
			"name":"FieldCount",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Field Count Is 3",
					"value":"3",
					"threshold":1.0,
					"error_on":"<",
					"silence":false,
					"use_count":false
				}
			]
		},
		{	
			"type":"Populated",
			"name":"ValuePopulated",
			"field":"value",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Value Field Is Populated",
					"value":"true",
					"threshold":1.0,
					"error_on":"<",
					"silence":false,
					"use_count":false
				}
			]
		},
		{
			"type":"DateFormat",
			"name":"ValidDate",
			"field":"date",
			"format":"2006-01-02 15:04:05",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Correct Date Format",
					"value":"true",
					"threshold":1.0,
					"error_on":"<",
					"silence":false,
					"use_count":false
				}
			]
		},
		{
			"type":"FutureDate",
			"name":"FutureDate",
			"field":"date",
			"format":"2006-01-02 15:04:05",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"No Future Dates",
					"value":"true",
					"threshold":0.0,
					"error_on":">",
					"silence":false,
					"use_count":false
				}
			]
		},
		{
			"type":"RegexTest",
			"name":"ValueIsAlpha",
			"field":"value",
			"regex":"[a-zA-Z]+",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Value Field is Alpha",
					"value":"true",
					"threshold":1.0,
					"error_on":"<",
					"silence":false,
					"use_count":false
				}
			]
		},
		{
			"type":"NumRecords",
			"name":"NumRecords",
			"evaluators":[
				{
					"evaluator":"VolumeTolerance",
					"description":"Number of Records",
					"min":4,
					"max":6
				}
			]
		},
		{
			"type":"ValidValues",
			"name":"ValidValues",
			"field":"value",
			"values":[
				"a","b","c","d","e"
			],
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Value is [a-e]",
					"value":"true",
					"threshold":1.0,
					"error_on":"<",
					"silence":false,
					"use_count":false
				}
			]
		},
		{
			"type":"Mean",
			"name":"Mean",
			"field":"id",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"Mean Is 3",
					"value":"mean",
					"threshold":3.0,
					"error_on":"<>",
					"silence":false,
					"use_count":true
				}
			]
		},
		{
			"type":"Percentile",
			"name":"Percentile",
			"field":"id",
			"evaluators":[
				{
					"evaluator":"ValueThreshold",
					"description":"65th Percentile is 4",
					"value":"65",
					"threshold":4.0,
					"error_on":"<>",
					"silence":false,
					"use_count":true
				}
			]
		},
		{
			"type":"CDF",
			"name":"CDF",
			"field":"id",
			"evaluators":[
				{
					"evaluator":"NearestValueThreshold",
					"description":"Cumulative density at 4.5 is less than 90%",
					"value":"4.5",
					"threshold":90,
					"error_on":">",
					"silence":false
				}
			]
		},
		{
			"type":"CDF",
			"name":"CDF",
			"field":"id",
			"evaluators":[
				{
					"evaluator":"NearestValueTolerance",
					"description":"Cumulative density at 4.5 is between 70% and 90%",
					"value":"4.5",
					"min":70,
					"max":90,
					"error_on":">",
					"silence":false
				}
			]
		},
		{
			"type":"NumericDistribution",
			"name":"NumericDistribution",
			"field":"id",
			"evaluators":[
				{
					"evaluator":"KS",
					"description":"KS is less than 10",
					"threshold":10.0,
					"benchmark":[
						{"key":"1", "value":2},
						{"key":"2", "value":1},
						{"key":"3", "value":1},
						{"key":"4", "value":2},
						{"key":"5", "value":1}
					],
					"error_on":">",
					"silence":false,
					"use_count":true
				}
			]
		}
	]
}
